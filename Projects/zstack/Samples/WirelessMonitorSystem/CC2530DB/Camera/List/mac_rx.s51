        MOV     DPTR,#0x6194
        MOV     A,#0x5
        MOVX    @DPTR,A
//  258 
//  259   /* clear any accidental threshold interrupt that happened as part of power up sequence */
//  260   MAC_RADIO_CLEAR_RX_THRESHOLD_INTERRUPT_FLAG();
        MOV     C,0xa8.7
